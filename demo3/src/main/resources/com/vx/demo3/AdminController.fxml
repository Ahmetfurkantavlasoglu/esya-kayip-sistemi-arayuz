<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.image.ImageView?>

<StackPane xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.vx.demo3.AdminController">
    <!-- Admin sayfasındaki içerikler -->
    <ImageView fx:id="imageView" fitHeight="150.0" fitWidth="150.0" layoutX="50.0" layoutY="50.0" />

    <!-- ListView eşyaları göstermek için -->
    <ListView fx:id="itemListView" layoutX="30.0" layoutY="150.0" prefHeight="200.0" prefWidth="300.0" />
</StackPane>